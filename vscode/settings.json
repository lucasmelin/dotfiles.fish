{
	"editor.fontSize": 16,
	"editor.fontLigatures": true,
	"[python]": {
		"editor.rulers": [
			80,
			88
		]
	},
	"files.watcherExclude": {
		"**/.nox/*": true,
		"**/.pytest_cache/**": true
	},
	"editor.tabSize": 4,
	"editor.trimAutoWhitespace": true,
	"editor.tabCompletion": "on",
	"editor.fontFamily": "'JetBrainsMono Nerd Font', 'Courier New', monospace",
	"editor.smoothScrolling": true,
	"editor.formatOnSave": true,
	"editor.inlineSuggest.enabled": true,
	"editor.formatOnType": true,
	"editor.cursorBlinking": "phase",
	"editor.suggestSelection": "first",
	"editor.minimap.enabled": false,
	"editor.renderWhitespace": "boundary",
	"explorer.openEditors.visible": 0,
	"window.restoreWindows": "none",
	"files.autoSave": "onFocusChange",
	"extensions.autoUpdate": true,
	"window.title": "${activeEditorMedium}${separator}${rootName}",
	"files.trimTrailingWhitespace": true,
	"extensions.ignoreRecommendations": false,
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"/vendor": true
	},
	"workbench.startupEditor": "newUntitledFile",
	"workbench.fontAliasing": "auto",
	"workbench.editor.showIcons": true,
	"explorer.confirmDragAndDrop": false,
	"search.smartCase": true,
	"files.associations": {
		"*.tmpl": "gotemplate"
	},
	"breadcrumbs.enabled": true,
	"telemetry.telemetryLevel": "off",
	"search.useGlobalIgnoreFiles": true,
	"workbench.editor.highlightModifiedTabs": true,
	"search.showLineNumbers": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"window.newWindowDimensions": "maximized",
	"go.formatTool": "gofumpt",
	"go.useCodeSnippetsOnFunctionSuggest": true,
	"go.autocompleteUnimportedPackages": true,
	"go.coverOnSingleTest": true,
	"go.useLanguageServer": true,
	"go.coverageDecorator": {
		"type": "gutter"
	},
	"go.alternateTools": {
		"go-langserver": "gopls"
	},
	"go.toolsManagement.autoUpdate": true,
	"go.buildFlags": [
		"-v"
	],
	"go.testTimeout": "3m",
	"go.testEnvVars": {
		"IS_NO_COLOR": "true"
	},
	"go.testFlags": [
		"-test.failfast",
		"-v"
	],
	"files.exclude": {
		"yarn.lock": true,
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true
	},
	"workbench.preferredDarkColorTheme": "Dracula",
	"files.insertFinalNewline": true,
	"workbench.colorTheme": "Dracula",
	"workbench.sideBar.location": "right",
	"debug.console.fontSize": 16,
	"diffEditor.codeLens": true,
	"remote.SSH.remotePlatform": {
		"darkstar.local": "linux"
	},
	"remote.SSH.remoteServerListenOnSocket": true,
	"workbench.editor.untitled.hint": "hidden",
	"github-actions.org-features": true,
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter.notebook.ipynb"
	},
	"security.workspace.trust.untrustedFiles": "open",
	"terminal.integrated.fontFamily": "'JetBrainsMono Nerd Font', 'Courier New', monospace",
	"terminal.external.osxExec": "iTerm.app",
	"terminal.integrated.fontSize": 16,
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.inheritEnv": true,
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.defaultProfile.osx": "fish",
	"go.delveConfig": {
		"dlvLoadConfig": {
			"followPointers": true,
			"maxVariableRecurse": 1,
			"maxStringLen": 64,
			"maxArrayValues": 64,
			"checkGoVersion": false,
			"maxStructFields": -1
		},
		"apiVersion": 2,
		"showGlobalVariables": false,
		"debugAdapter": "legacy",
		"substitutePath": []
	},
	"html.format.endWithNewline": true,
	"html.format.indentHandlebars": true,
	"html.format.templating": true,
	"explorer.confirmDelete": false,
	"[dockerfile]": {
		"editor.defaultFormatter": "ms-azuretools.vscode-docker"
	},
	"security.workspace.trust.banner": "never",
	"security.workspace.trust.startupPrompt": "never",
	"security.workspace.trust.enabled": false,
	"editor.bracketPairColorization.enabled": true,
	"git.autofetch": true,
	"terminal.integrated.defaultProfile.linux": "fish"
}
